                    <span class="third">
                        @Html.LabelFor(model => model.ContractDate) @Html.ValidationMessageFor(model => model.ContractDate)
                        @Html.TextBoxFor(model => model.ContractDate, new { data_bind = "value: ContractDate", @class = "text ui-widget-content ui-corner-all date-pick" })
