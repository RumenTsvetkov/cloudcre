<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>@ViewBag.Title</title>

        @MvcHtmlString.Create(SquishIt.Framework.Bundle.Css()
            .Add("~/Content/Bootstrap/css/bootstrap.min.css")
            .Add("~/Content/Bootstrap/css/site.css")
            .Add("~/Content/css/reset.css")
            .Add("~/Content/css/icons.css")
            .Add("~/Content/css/workspace.css")
            .AddRemote("~/Content/jquery-ui-1.8.18.css", "http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/themes/redmond/jquery-ui.css")
            .Render("~/Content/ccre_layout2#.css"))
                    
    <style type="text/css">
      body {
        padding-top: 60px;
        padding-bottom: 40px;
      }
      .sidebar-nav {
        padding: 9px 0;
      }
    </style>

    <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="//html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
        <!-- Le fav and touch icons -->

        <script src="http://maps.google.com/maps/api/js?v=3.3&sensor=false" type="text/javascript"></script>
        <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
        <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.17/jquery-ui.min.js" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/Bootstrap/bootstrap.min.js")" type="text/javascript"></script>
        
        @*// TODO: replace squishit with better compression utility*@
        <script src="@Url.Content("~/Scripts/modernizr-2.5.3.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/Bootstrap/bootstrap.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/JqueryTemplates/jquery.tmpl.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/JqueryCookie/jquery.cookie.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/DigitalBushMaskedInput/jquery.maskedinput-1.2.2.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/NumberFormatter/jshashtable-2.1.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/NumberFormatter/jquery.numberformatter-1.2.2.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/CrockfordJson2/json2.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/Jmapping/markermanager.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/Jmapping/StyledMarker.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/Jmapping/jquery.metadata.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/Jmapping/jquery.jmapping.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/Knockout/knockout-1.3.0beta.debug.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/Knockout/knockout.mapping-latest.debug.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/JqueryForm/jquery.form.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/JqueryMenu/fg.menu.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/DataTables/jquery.dataTables.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/Underscore/underscore-1.1.5.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/Backbone/backbone-0.5.0.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/VisualSearch/visualsearch.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/VisualSearch/views/search_box.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/VisualSearch/views/search_facet.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/VisualSearch/views/search_input.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/VisualSearch/models/search_facets.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/VisualSearch/models/search_query.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/VisualSearch/utils/backbone_extensions.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/VisualSearch/utils/hotkeys.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/VisualSearch/utils/jquery_extensions.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/VisualSearch/utils/search_parser.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/VisualSearch/utils/inflector.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Libs/VisualSearch/templates/templates.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Scripts/Views/Layout.js")" type="text/javascript"></script>
    </head>
    <body>
        @if (TempData["message"] != null)
        {
            <div class="Message">@TempData["message"]</div>
        }
        
        @if (IsSectionDefined("siteHeader"))
        {
            @RenderSection("siteHeader")
        }
        else 
        {           
            
            <div class="navbar navbar-fixed-top">
              <div class="navbar-inner">
                <div class="container-fluid">
                  <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                  </a>
                  <a class="brand" href="#">Cloud CRE</a>
                  <div class="nav-collapse">
                    <ul class="nav">
@*                      <li class="active"><a href="#">Home</a></li>
                      <li><a href="#about">About</a></li>
                      <li><a href="#contact">Contact</a></li>*@
                    </ul>
                    <p class="navbar-text pull-right">@Context.User.Identity.Name&nbsp;|@Html.ActionLink("Sign Out", "LogOff", "Account", null, new { rel = "nofollow" })</p>
                  </div><!--/.nav-collapse -->
                </div>
              </div>
            </div>
        }
        <section id="main">
            @RenderBody()
        </section>
        <footer></footer>
