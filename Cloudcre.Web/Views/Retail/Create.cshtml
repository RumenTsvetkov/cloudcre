@model Cloudcre.Service.Property.ViewModels.RetailViewModel

@{
    ViewBag.Title = "Cloud CRE - Create new retail property";
    Layout = "~/Views/Shared/Property/_Layout_Property_Create.cshtml";
}

@section scripts {
    <script type="text/javascript">
        window.cloudcre.routing.urls({
            action: '@Url.Action("Create", "Retail")',
            model: '@Url.Action("Model", "Retail")'
        });
    </script>
    <script type="text/html" id="template">
        @using (Html.BeginForm("Create", "Retail", FormMethod.Post, new { id = "dialog-form", @class = "wufoo" }))
        {
            @Html.Partial("_Template")
        }
    </script>
}